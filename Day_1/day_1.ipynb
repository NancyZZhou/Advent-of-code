   "source": [
    "import numpy as np\n",
    "\n",
    "with open('input.txt', 'r') as f:\n",
    "    data = f.read().replace('\\n', ',').split(',,')\n",
    "\n",
    "calories = np.array(list(sum(list(map(int, elf.split(',')))) for elf in data))\n",
    "\n",
    "print(calories[calories.argmax()])"
   ]
  }
